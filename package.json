{
  "name": "my-fullstack-app",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
 "scripts": {
    "install:all": "npm ci && npm run install:frontend && npm run install:backend",
    "install:frontend": "cd frontend && npm ci",
    "install:backend": "cd backend && npm ci",
    "build:all": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "start:all": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "cd frontend && npm start",
    "start:backend": "cd backend && node server.js"
  },
  "dependencies": {
    "concurrently": "^6.0.0"
  },
  "devDependencies": {

  }
}
